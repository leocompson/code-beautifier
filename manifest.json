{
  "version": "0.1.3",
  "manifest_version": 3,
  "offline_enabled": true,
  "short_name": "code-beautifier",
  "name": "Code Beautifier (JS, CSS, HTML)",
  "permissions": ["storage", "contextMenus"],
  "homepage_url": "https://mybrowseraddon.com/code-beautifier.html",
  "description": "A lite and free JavasScript, CSS and HTML beautifier for your browser.",
  "commands": {
    "_execute_action": {}
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Code Beautifier",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
